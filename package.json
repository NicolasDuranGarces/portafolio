{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -noEmit && vite build",
    "preview": "vite preview --port 5173",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
